{
  "name": "@byc/tipc",
  "type": "module",
  "version": "0.0.93",
  "packageManager": "pnpm@9.15.9",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "dev": "./core/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./main": {
      "dev": "./core/main.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./preload": {
      "dev": "./core/preload.ts",
      "import": "./dist/preload.js",
      "require": "./dist/preload.cjs"
    },
    "./type": {
      "dev": "./core/type.ts",
      "import": "./dist/type.js",
      "require": "./dist/type.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts",
        "./dist/index.d.cts"
      ],
      "main": [
        "./dist/main.d.ts",
        "./dist/main.d.cts"
      ],
      "preload": [
        "./dist/preload.d.ts",
        "./dist/preload.d.cts"
      ],
      "type": [
        "./dist/type.d.ts",
        "./dist/type.d.cts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky || true",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "dev": "run-p build play",
    "play": "pnpm --filter ./playground dev",
    "build": "tsup",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "pkg:publish": "pnpm build & pnpm publish",
    "pkg:publish:test": "pnpm build & pnpm publish --no-git-checks --dry-run",
    "pkg:publish:window-tipc": "pnpm --filter @byc/window-tipc pkg:publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "@antfu/ni": "^24.3.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@swc/core": "^1.11.11",
    "@types/node": "^22.13.11",
    "@vitest/coverage-v8": "2.1.4",
    "electron": "^33.4.6",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "tsup": "^8.4.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
